   "source": [
    "from itertools import accumulate\n",
    "def connectSticks(sticks):\n",
    "    if len(sticks) == 1: return 0\n",
    "    sticks = list(accumulate(sorted(sticks)))\n",
    "    return sum(sticks[1:])"
   ]
  },
