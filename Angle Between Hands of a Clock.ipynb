    "def angleClock(hour, minutes):\n",
    "    hour = (hour % 12) * 60 + minutes\n",
    "    hour_deg = hour / 2\n",
    "    minute_deg = minutes * 6\n",
    "    angle = abs(hour_deg - minute_deg)\n",
    "    return angle if angle <= 180 else round(360 - angle, 5)"
   ]
  },
