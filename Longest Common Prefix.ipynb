   "source": [
    "def longestCommonPrefix(strs):\n",
    "    res = \"\"\n",
    "    \n",
    "    for i in range(len(strs[0])):\n",
    "        for s in strs:\n",
    "            if i == len(s) or s[i] != strs[0][i]:\n",
    "                return res\n",
    "        res += strs[0][i]\n",
    "    \n",
    "    return res"
   ]
