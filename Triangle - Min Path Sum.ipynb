    "def minimumTotal(self, triangle):\n",
    "    dp = [0]*(len(triangle) + 1)\n",
    "        \n",
    "    for row in triangle[::-1]:\n",
    "        for i, n in enumerate(row):\n",
    "            dp[i] = n + min(dp[i], dp[i+1])\n",
    "        \n",
    "    return dp[0]"
