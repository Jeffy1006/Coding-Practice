    "def sumOfLeftLeaves(root):\n",
    "    if not root:\n",
    "        return 0\n",
    "    elif root.left and root.left.left and root.left.right:\n",
    "        return root.left.val + sumOfLeftLeaves(root.right)\n",
    "    else:\n",
    "        return sumOfLeftLeaves(root.left) + sumOfLeftLeaves(root.right)"
   ]
