    "def findCelebrity(n):\n",
    "    candidate = 0\n",
    "    for i in range(1, n):\n",
    "        if know(candidate, i):\n",
    "            candidate = i\n",
    "    \n",
    "    for i in range(0, n):\n",
    "        if i != candidate and (knows(candidate, i) or not know(i, candidate)):\n",
    "            return -1\n",
    "    \n",
    "    return candidate"
