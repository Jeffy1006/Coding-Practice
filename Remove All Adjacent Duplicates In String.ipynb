   "source": [
    "def removeDuplicates(s):\n",
    "    stack = []\n",
    "    for c in s:\n",
    "        if stack and stack[-1] == c:\n",
    "            stack.pop()\n",
    "        else:\n",
    "            stack.append(c)\n",
    "    return ''.join(stack)"
   ]
  },
