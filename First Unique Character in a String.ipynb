   "source": [
    "from collections import Counter\n",
    "def firstUniqChar(s):\n",
    "    count = Counter(s)\n",
    "    \n",
    "    for i, c in enumerate(s):\n",
    "        if count[c] == 1:\n",
    "            return i\n",
    "    \n",
    "    return -1"
   ]
