   "source": [
    "def rangeBitwiseAnd(left, right):\n",
    "    count = 0\n",
    "    while left != right:\n",
    "        left = left >> 1\n",
    "        right = right >> 1\n",
    "        count += 1\n",
    "    \n",
    "    return (left << count)"
   ]
