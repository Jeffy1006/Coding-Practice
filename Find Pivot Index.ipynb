    "def pivotIndex(nums):\n",
    "    left, right = 0, sum(nums)\n",
    "    for i, num in enumerate(nums):\n",
    "        right -= num\n",
    "        if right == left:\n",
    "            return i\n",
    "        left += num\n",
    "    return -1"
   ]
