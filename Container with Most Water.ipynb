   "source": [
    "def maxArea(height):\n",
    "    res = 0\n",
    "    l, r = 0, len(height) - 1\n",
    "    \n",
    "    while l < r:\n",
    "        area = (r - l) * min(height[l], height[r])\n",
    "        res = max(res, area)\n",
    "        if height[l] < height[r]:\n",
    "            l += 1\n",
    "        else:\n",
    "            r -= 1\n",
    "    \n",
    "    return res"
   ]
