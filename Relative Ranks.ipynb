   "source": [
    "def relativeRanks(nums):\n",
    "    sort = sorted(nums)[::-1]\n",
    "    rank = ['Gold Medal', 'Silver Medal', 'Bronze Medal'] + [str(i) for i in range(4, len(nums) + 1)]\n",
    "    return list(map(dict(zip(sort, rank)).get, nums))"
   ]
  },
