    "def numTrees(n):\n",
    "    \n",
    "    if n < 2:\n",
    "        return 1\n",
    "    \n",
    "    sol = [0] * (n+1)\n",
    "    sol[0], sol[1] = 1, 1\n",
    "    for i in range(2,n+1):\n",
    "        for j in range(i):\n",
    "            sol[i] += sol[j]*sol[i-j-1]\n",
    "    \n",
    "    return sol[n]\n",
    "    \n",
    "    "
   ]
  }
