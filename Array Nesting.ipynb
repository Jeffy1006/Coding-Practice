   "source": [
    "def arrayNesting(nums):\n",
    "    seen, res = [0] * len(nums), 0\n",
    "    for i in nums:\n",
    "        cnt = 0\n",
    "        while not seen[i]:\n",
    "            seen[i], cnt, i = 1, cnt + 1, nums[i]\n",
    "        res = max(res, cnt)\n",
    "    return res"
   ]
