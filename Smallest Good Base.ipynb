    "import math\n",
    "def smallestGoodBase(n):\n",
    "    n = int(n)\n",
    "    max_m = int(math.log(n, 2))\n",
    "    for m in range(max_m, 1, -1):\n",
    "        k = int(n**m**-1)\n",
    "        if (k**(m + 1) - 1) // (k - 1) == n:\n",
    "            return str(k)\n",
    "    return str(n - 1)"
   ]
