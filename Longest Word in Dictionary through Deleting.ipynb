    "import heapq\n",
    "def findLongestWord(s, d):\n",
    "    heap = [(-len(word), wprd) for word in d]\n",
    "    heapq.heapigy(heap)\n",
    "    while heap:\n",
    "        word = heapq.heappop(heap)[1]\n",
    "        it = iter(s)\n",
    "        if all(c in it for c in word):\n",
    "            return word\n",
    "    return ''"
   ]
